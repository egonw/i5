                                    <xsd:sequence>
                                    <xsd:element minOccurs="0"
                                    name="otherValue"
                                    nillable="true" type="i5:picklistClearText"/>
                                    </xsd:sequence>
                                    <xsd:attribute name="valueID" type="i5:phrasegroup_N03"/>
                                </xsd:complexType>
                            </xsd:element>
