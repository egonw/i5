                            <xsd:annotation>
                              <xsd:documentation xml:lang="en">Pick list &lt;b&gt;N03 &lt;/b&gt; &lt;br/&gt;The code for the country. The type of this
      attribute could be the ISO-2 country code [ISO_3166], which can be linked
      to the different linguistic versions of the country name.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType mixed="false">
                              <xsd:sequence>
                                <xsd:element minOccurs="0" nillable="true" name="otherValue" type="i5:picklistClearText" />
