        <xsd:element name="ghs" minOccurs="0">
          <xsd:complexType mixed="false">
            <xsd:sequence>
              <xsd:element maxOccurs="unbounded" minOccurs="0" name="ghs">
                <xsd:complexType mixed="false">
                  <xsd:sequence>
                    <xsd:element name="dataProtection" minOccurs="0">
